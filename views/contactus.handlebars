<section id="contactform">
    <div class="container mt-5 mb-5 pb-3 pt-3">
        <div class="row">
            <div class="col-6 mt-5">
                <img src="../public/images/logo/Asset 25.svg" class="w-50" id="mhsicon" alt="logo">
                <h4 class="darkgold-text mt-5 mb-5 bold">Myanmar High Society Co., Ltd</h4>
                <div class="media mt-3 mb-3">
                    <div class="media-left media-middle f-20 pr-3 pt-3">
                        <i class="fa fa-institution"></i>
                    </div>
                    <div class="media-body f-20">
                        <p>Room no.189, 18th Floor, Orchid Condominium, Ahlone Township, Yangon, Myanmar.</p>
                    </div>
                </div>
                <div class="media mt-3 mb-3">
                    <div class="media-left media-middle f-20 pr-3">
                        <i class="fa fa-phone"></i>
                    </div>
                    <div class="media-body f-20">
                        <p>+9595231938, +959450880001</p>
                    </div>
                </div>
                <div class="media mt-3 mb-3">
                    <div class="media-left media-middle f-20 pr-3">
                        <i class="fa fa-envelope"></i>
                    </div>
                    <div class="media-body f-20">
                        <p>Sale: hello@myanmarhighsociety.com</p>
                    </div>
                </div>
            </div>
            <div class="col-6 mt-4">
                <form id="contact-form" method="post" action="send" role="form">
                    <div class="messages"></div>
                    <div class="form-group row">
                        <div class="col-md-10">
                            <input type="text" class="form-control lg-input" name="name" id="name"
                                placeholder="Your Name" required data-error="Name is required.">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>
                    <div class="form-group row mt-4">
                        <div class="col-md-10">
                            <input type="email" class="form-control lg-input" name="email" id="email"
                                placeholder="example@gmail.com" required data-error="Valid email is required.">
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>
                    <div class="form-group row mt-4">
                        <div class="col-md-10">
                            <textarea class="form-control lg-input" rows="5" name="comment" id="comment"
                                placeholder="Comment *" required data-error="Please, leave us a message."></textarea>
                            <div class="help-block with-errors"></div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="g-recaptcha" data-sitekey="6LeL9toUAAAAAN17UelAE_cjHTL4djWmgQd7DOfr"
                            data-callback="verifyRecaptchaCallback" data-expired-callback="expiredRecaptchaCallback" y>
                        </div>
                        <input class="form-control d-none" data-recaptcha="true" required
                            data-error="Please complete the Captcha">
                        <div class="help-block with-errors"></div>
                    </div>
                    <div class="form-group row mt-4">
                        <div class="col-md-4">
                            <button type="submit" class="btn btn-darkgold text-white form-control">
                                <i class="fa fa-paper-plane text-white" aria-hidden="true"></i>
                                Send
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

