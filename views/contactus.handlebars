<script>
    function myMap() {
        var mapProp = {
            center: new google.maps.LatLng(16.790591, 96.131949),
            zoom: 15,
        };
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
    }

    window.addEventListener("hashchange", function () {
        window.scrollTo(window.scrollX, window.scrollY - 100);
    });

    var navbar = $('.headernav');

    $(window).scroll(function () {
        var scroll = $(window).scrollTop();
        if (scroll >= 20) {
            navbar.addClass('header-small');
            navbar.addClass('header-shadow');
            navbar.removeClass('ftco-navbar-light');
            navbar.addClass('ftco-navbar-blight');
        } else {
            navbar.removeClass('header-small');
            navbar.removeClass('header-shadow');
            navbar.addClass('ftco-navbar-light');
            navbar.removeClass('ftco-navbar-blight');
        }
    }).scroll();

    function openNav() {
        document.getElementById("mySidenav").style.width = "100%";
    }

    function closeNav() {
        document.getElementById("mySidenav").style.width = "0";
    }

    $(document).ready(function () {

        window.verifyRecaptchaCallback = function (response) {
            $('input[data-recaptcha]').val(response).trigger('change');
        }

        window.expiredRecaptchaCallback = function () {
            $('input[data-recaptcha]').val("").trigger('change');
        }

        $('#contact-form').validator();

        $('#contact-form').submit(function (e) {
            if (!e.isDefaultPrevented()) {
                var loadingText = "<i class='fa fa-spinner fa-spin text-white'></i> Sending...";
                if ($('.btn-darkgold').html() !== loadingText) {
                    $('.btn-darkgold').data('original-text', $('.btn-darkgold').html());
                    $('.btn-darkgold').html(loadingText);
                }
                $(this).ajaxSubmit({
                    error: function (xhr) {
                        setTimeout(function () {
                            var messageAlert = 'alert-' + response.responseDesc;
                            var messageText = response.msg;

                            var alertBox = '<div class="alert ' + messageAlert + ' alert-dismissable"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>' + messageText + '</div>';
                            if (messageAlert && messageText) {
                                $('#contact-form').find('.messages').html(alertBox);
                                $('.btn-darkgold').html($('.btn-darkgold').data('original-text'));
                                $('#contact-form')[0].reset();
                                grecaptcha.reset();
                            }
                        }, 2000);
                    },
                    success: function (response) {
                        setTimeout(function () {
                            var messageAlert = 'alert-' + response.responseDesc;
                            var messageText = response.msg;

                            var alertBox = '<div class="alert ' + messageAlert + ' alert-dismissable"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>' + messageText + '</div>';
                            if (messageAlert && messageText) {
                                $('#contact-form').find('.messages').html(alertBox);
                                $('.btn-darkgold').html($('.btn-darkgold').data('original-text'));
                                $('#contact-form')[0].reset();
                                grecaptcha.reset();
                            }
                        }, 3000);
                    }
                });
                //Very important line, it disable the page refresh.
            }
            return false;
        });
    });

</script>
